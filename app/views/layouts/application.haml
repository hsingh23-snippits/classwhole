<!DOCTYPE html>
%html
  %head
    %title Classwhole
    = stylesheet_link_tag    "http://twitter.github.com/bootstrap/1.3.0/bootstrap.min.css"
    = stylesheet_link_tag    "application" 
    = csrf_meta_tags
    = javascript_tag          "var AUTH_TOKEN = #{form_authenticity_token.inspect};" if protect_against_forgery? 
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js" 
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js" 
    = javascript_include_tag "http://twitter.github.com/bootstrap/1.3.0/bootstrap-modal.js"
    = javascript_include_tag "http://twitter.github.com/bootstrap/1.3.0/bootstrap-alerts.js"
    = javascript_include_tag "general/alert" 
  %body
    %div#wrapper
      = render "shared/header"
      = yield
